{
  "name": "lilnouns-vault",
  "version": "1.0.0-beta.11",
  "description": "A template for create smart contracts using Foundry and Hardhat.",
  "keywords": [],
  "license": "Apache-2.0",
  "author": "Milad Nekofar <milad@nekofar.com>",
  "directories": {
    "lib": "lib",
    "test": "test"
  },
  "scripts": {
    "prebuild": "pnpm run lint",
    "build": "pnpm run --sequential '/^build:.*/'",
    "build:forge": "forge build --sizes",
    "build:hardhat": "hardhat compile",
    "clean": "pnpm run --sequential '/^clean:.*/'",
    "clean:forge": "forge clean",
    "clean:hardhat": "hardhat clean",
    "preinstall": "npx -y only-allow pnpm",
    "postinstall": "forge install && pnpm remappings",
    "lint": "pnpm run --sequential '/^lint:.*/'",
    "lint:hardhat": "hardhat check",
    "lint:slither": "command -v slither >/dev/null 2>&1 || pip install slither-analyzer; slither .",
    "prepare": "husky",
    "remappings": "forge remappings > remappings.txt",
    "pretest": "pnpm run build",
    "test": "pnpm run --sequential '/^test:.*/'",
    "test:forge": "forge test -vvvv",
    "test:hardhat": "hardhat test --typecheck"
  },
  "resolutions": {
    "cookie": ">=0.7.0",
    "debug": ">=3.2.7",
    "elliptic": ">=6.6.0",
    "eth-gas-reporter": ">=0.2.26",
    "flat": ">=5.0.1",
    "minimatch": ">=3.0.5",
    "request": ">=2.88.2",
    "secp256k1": ">=4.0.4",
    "tough-cookie": ">=4.1.3",
    "word-wrap": ">=1.2.4",
    "ws": ">=7.5.10"
  },
  "dependencies": {
    "@openzeppelin/contracts": "5.3.0",
    "@openzeppelin/contracts-upgradeable": "5.3.0",
    "@openzeppelin/foundry-upgrades": "OpenZeppelin/openzeppelin-foundry-upgrades",
    "@openzeppelin/hardhat-upgrades": "3.9.0",
    "@openzeppelin/upgrades-core": "1.44.0",
    "ds-test": "dapphub/ds-test",
    "forge-std": "foundry-rs/forge-std",
    "solidity-stringutils": "github:Arachnid/solidity-stringutils"
  },
  "devDependencies": {
    "@nomicfoundation/hardhat-chai-matchers": "2.0.8",
    "@nomicfoundation/hardhat-ethers": "3.0.8",
    "@nomicfoundation/hardhat-foundry": "1.1.4",
    "@nomicfoundation/hardhat-network-helpers": "1.0.12",
    "@nomicfoundation/hardhat-toolbox": "5.0.0",
    "@nomiclabs/hardhat-solhint": "4.0.2",
    "@typechain/ethers-v6": "0.5.1",
    "@typechain/hardhat": "9.1.0",
    "@types/chai": "4.3.11",
    "@types/mocha": "10.0.10",
    "@types/node": "22.15.0",
    "chai": "4.5.0",
    "ethers": "6.13.5",
    "hardhat": "2.23.0",
    "husky": "9.1.7",
    "lint-staged": "15.5.1",
    "prettier": "3.5.3",
    "prettier-plugin-packagejson": "2.5.10",
    "prettier-plugin-solidity": "1.4.2",
    "prettier-plugin-toml": "2.0.4",
    "solhint": "5.0.5",
    "ts-node": "10.9.2",
    "typechain": "8.3.2",
    "typescript": "5.8.3"
  },
  "engines": {
    "node": ">=20.0.0",
    "pnpm": ">=9.0.0"
  },
  "packageManager": "pnpm@10.0.0"
}
